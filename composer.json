{
    "name": "zicht/itertools",
    "license": "MIT",
    "scripts": {
        "lint": [
            "phpcs -s src/Zicht/ tests/Zicht/ --standard=vendor/zicht/standards-php/src/Zicht --extensions=php -p --parallel=$(nproc)"
        ],
        "lint-fix": [
            "phpcbf -s src/Zicht/ tests/Zicht/ --standard=vendor/zicht/standards-php/src/Zicht --extensions=php --parallel=$(nproc)"
        ],
        "test": [
            "phpunit -c phpunit.xml.dist"
        ]
    },
    "description": "A collection of functions that mirror the Python itertools library",
    "authors": [
        {
            "name": "Zicht online",
            "email": "info@zicht.nl"
        }
    ],
    "require-dev": {
        "doctrine/collections": "^1.3",
        "phpunit/phpunit": "4.*",
        "twig/twig": "^1.30 || ^2.0",
        "zicht/standards-php": "^4.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": {
            "Zicht": "src/"
        },
        "files": [
            "src/Zicht/Itertools/conversions.php",
            "src/Zicht/Itertools/filters.php",
            "src/Zicht/Itertools/itertools.php",
            "src/Zicht/Itertools/mappings.php",
            "src/Zicht/Itertools/reductions.php"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "Zicht": "tests/"
        }
    },
    "require": {
    }
}
